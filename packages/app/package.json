{
  "name": "@lab/app",
  "version": "0.0.0",
  "private": true,
  "description": "project to reproduce issues with ts-jest and mts files",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test:ts-jest": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.ts-jest.config.mjs --no-cache",
    "test:swc": "jest --config jest.swc.config.mjs --no-cache",
    "clean": "rm -rf dist dist-test tsconfig.tsbuildinfo tsconfig.jest.tsbuildinfo",
    "build": "npm run clean && tsc",
    "build:test": "npm run clean && tsc --project tsconfig.jest.json",
    "test:plain": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.plain-jest.config.mjs --no-cache"
  },
  "dependencies": {
    "@lab/commons": "*"
  },
  "author": "",
  "license": "ISC"
}
