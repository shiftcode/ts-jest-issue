{
  "name": "@lab/commons",
  "version": "0.0.0",
  "private": true,
  "description": "project to reproduce issues with ts-jest and mts files",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test:ts-jest": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.ts-jest.config.mjs --no-cache",
    "test:swc": "jest --config jest.swc.config.mjs --no-cache",
    "clean": "rm -rf dist dist-test tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc",
    "build:test": "npm run clean && tsc --project tsconfig.jest.json"
  },
  "author": "",
  "license": "ISC",
  "exports": {
    ".": {
      "types":  "./dist/_types/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./helpers": {
      "types":  "./dist/_types/helpers/index.d.mts",
      "default": "./dist/helpers/index.mjs"
    }
  }

}
